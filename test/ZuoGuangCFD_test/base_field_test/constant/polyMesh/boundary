FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

3
(
    movingWall
    {
        type            wall;
        inGroups        1(wall);
        nFaces          4;
        startFace       24;
    }
    fixedWalls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          12;
        startFace       28;
    }
    frontAndBack
    {
        type            empty;
        inGroups        1(empty);
        nFaces          32;
        startFace       40;
    }
)

